{
    "manifest_version": 2,
    "name": "Signematic",
    "description": "This extension changes the background color.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/black white clean movie logo.png",
        "48": "images/black white clean movie logo.png",
        "128": "images/black white clean movie logo.png"
      }
    },
    "icons": {
      "16": "images/black white clean movie logo.png",
      "48": "images/black white clean movie logo.png",
      "128": "images/black white clean movie logo.png"
    }
  }
  